LOCATE COMP "clock" SITE "G2";
IOBUF PORT  "clock" PULLMODE=NONE IO_TYPE=LVCMOS33;
FREQUENCY PORT "clock" 15 MHZ;

LOCATE COMP "reset" SITE "D6";
IOBUF PORT  "reset" PULLMODE=UP IO_TYPE=LVCMOS33;

LOCATE COMP "UART0_tx" SITE "L4";
IOBUF PORT  "UART0_tx" PULLMODE=UP IO_TYPE=LVCMOS33 DRIVE=4;
LOCATE COMP "UART0_rx" SITE "M1";
IOBUF PORT  "UART0_rx" PULLMODE=UP IO_TYPE=LVCMOS33;

LOCATE COMP "led0" SITE "B2";         # LED0 (Heartbeat)
IOBUF PORT  "led0" PULLMODE=NONE IO_TYPE=LVCMOS33 DRIVE=4;

LOCATE COMP "GPIO0[0]" SITE "C2";     # LED1
IOBUF PORT  "GPIO0[0]" PULLMODE=NONE IO_TYPE=LVCMOS33 DRIVE=4;
